#include<stdio.h>
main()
{
	printf("food item : \n1. pizza,RS 239\n2. Burger,Rs129\n3. Pasta,Rs179\n4. French Fries,Rs99\n5. sandwich,Rs149");
	
	int choice=0;
	scanf("%d",&choice);
	
	switch(choice)
	{
		case 1:
			printf("food item -pizza price -Rs 239.");
			break;
		case 2:
			printf("food item -Burger price -Rs 129.");
			break;
		case 3:
			printf("food item -pasta price - Rs 179.");
			break;
		case 4:
			printf("food item -French fries price -Rs 99.");
			break;
		case 5:
			printf("food item - Sandwich price -Rs 149.");
			break;
		default : printf("Invalid choice");
	
	}

}
